algoritmo "semnome"
var
   nome, nomeM, nomeP, cidade, endereco: caractere
   rg, idade, cpf: inteiro
   altura, renda, peso: real
inicio

      escreval("Digite seu nome: ")
      leia(nome)
      escreval("Digite sua idade: ")
      leia(idade)
      escreval("Digite seu peso: ")
      leia(peso)
      escreval("Digite sua altura: ")
      leia(altura)
      escreval("Digite a cidade em que você mora: ")
      leia(cidade)
      escreval("Digite seu endereço: ")
      leia(endereco)
      escreval("Digite seu RG: ")
      leia(rg)
      escreval("Digite seu cpf: ")
      leia(cpf)
      escreval("Digite o nome da sua mãe: ")
      leia(nomeM)
      escreval("Digite o nome do seu pai: ")
      leia(nomeP)
      escreval("Digite sua renda bruta: ")
      leia(renda)
      
      escreval("NOME: ", nome)
      escreval("IDADE: ", idade)
      escreval("PESO: ", peso,"kg")
      escreval("ALTURA: ", altura)
      escreval("CIDADE: ", cidade)
      escreval("ENDEREÇO: ", endereco)
      escreval("RG: ", rg)
      escreval("CPF: ", cpf)
      escreval("NOME DA MÃE: ", nomeM)
      escreval("NOME DO PAI: ", nomeP)
      escreval("RENDA BRUTA", renda)
